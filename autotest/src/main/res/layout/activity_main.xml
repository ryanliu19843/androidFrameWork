<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="item"
            type="com.mdx.framework.server.api.base.Msg_Key" />

        <variable
            name="clickListener"
            type="android.view.View.OnClickListener" />

        <import type="com.mdx.framework.autofit.AutoFitUtil" />
    </data>

    <com.mdx.framework.autofit.layout.FitScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/content_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingBottom="@dimen/activity_vertical_margin"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            tools:context="com.example.autotest.MainActivity"
            tools:showIn="@layout/activity_main">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/titleAct"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="TitleAct" />

                <Button
                    android:id="@+id/titleTransparentAct"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="TitleTransparentAct" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/noTitleAct"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="NoTitleAct" />

                <Button
                    android:id="@+id/titleTransStatusAct"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="TitleTransStatusAct" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/indexact"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="IndexAct" />

                <Button
                    android:id="@+id/loadingact"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="Loading" />

                <Button
                    android:id="@+id/autofittest"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="autofittest" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/photoslec"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="获取照片" />

                <Button
                    android:id="@+id/startmain"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="startmain" />
            </LinearLayout>

            <com.mdx.framework.widget.banner.CirleCurr
                android:id="@+id/ciclecurr"
                android:layout_width="match_parent"
                android:layout_height="100dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/stop"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="停止" />

                <Button
                    android:id="@+id/start"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="开始" />

                <Button
                    android:id="@+id/pause"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="暂停" />

                <Button
                    android:id="@+id/setloop"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/j40dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="循环" />
            </LinearLayout>

            <com.mdx.framework.widget.MImageView
                android:id="@+id/image"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:clickable="true"
                android:scaleType="fitXY"
                app:obj="ASSETS:tylt_bg_youdoushi_n.png" />
        </LinearLayout>
    </com.mdx.framework.autofit.layout.FitScrollView>
</layout>

