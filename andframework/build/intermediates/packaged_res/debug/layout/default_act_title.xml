<?xml version="1.0" encoding="UTF-8"?>
<com.mdx.framework.widget.suit.SLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/defautlt_framecontainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/topWidget"
        style="@style/TitleStatus"
        android:layout_width="match_parent"
        android:layout_height="0dp" />

    <com.mdx.framework.widget.ActionBar
        android:id="@+id/ActionbarWidget"
        style="@style/TitleAction" />

    <FrameLayout
        android:id="@+id/defautlt_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <View
        android:id="@+id/bottomWidget"
        style="@style/TitleNavigation"
        android:layout_width="match_parent"
        android:layout_height="0dp" />
</com.mdx.framework.widget.suit.SLinearLayout>

